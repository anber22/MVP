{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/app/proxy"
    },
    {
      "source": "/mvp/(.*)",
      "destination": "https://back-end-api.aiproshots.com/$1"
    },
    {
      "source": "/img/(.*)",
      "destination": "https://aiproshots-image.s3.amazonaws.com/$1"
    }
  ],
  "functions": {
    "api/proxy.js": {
      "maxDuration": 300
    }
  }
}