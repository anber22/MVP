{
  "functions": {
    "api/*": {
      "maxDuration": 300
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "headers": {
        "Timeout": "300000"
      },
      "dest": "https://stable-diffsuion.aiproshots.com/$1"
    },
    {
      "src": "/mvp/(.*)",
      "dest": "https://back-end-api.aiproshots.com/$1"
    },
    {
      "src": "/img/(.*)",
      "dest": "https://aiproshots-image.s3.amazonaws.com/$1"
    }
  ]
}