{
  "rewrites": [
    {
      "source": "/api/:match*",
      "destination": "https://stable-diffsuion.aiproshots.com/:match*"
    },
    {
      "source": "/mvp/:match*",
      "destination": "https://back-end-api.aiproshots.com/:match*"
    },
    {
      "source": "/img/:match*",
      "destination": "https://aiproshots-image.s3.amazonaws.com/:match*"
    }
  ],
  "experimental": {
    "proxyTimeout": 300000
  }
}